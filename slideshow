<button type="button" class="btn btn-default btn-arrow-left" style="position:absolute;top:45%;left:0;background:#e9b50b" onclick="plusDivs(-1)"></a>left</button>
<button type="button" class="btn btn-default btn-arrow-right" style="position:absolute;top:45%;right:0;background:#e9b50b" onclick="plusDivs(1)"></a>right</button>


</div>

<script>
var slideIndex = 1;
showDivs(slideIndex);

function plusDivs(n) {
  showDivs(slideIndex += n);
}

function showDivs(n) {
  var i;
  var x = document.getElementsByClassName("mySlides");
  if (n > x.length) {slideIndex = 1}    
  if (n < 1) {slideIndex = x.length}
  for (i = 0; i < x.length; i++) {
     x[i].style.display = "none";  
  }
  x[slideIndex-1].style.display = "block";  
}
</script>

